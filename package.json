{
  "name": "getnet",
  "version": "1.0.1",
  "description": "A library/abstraction in Node.JS to implements the Getnet's services.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && node_modules/.bin/babel src -d lib --ignore '**/*.test.js,src/test' --verbose",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --passWithNoTests"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "camelcase-keys": "^6.2.2",
    "qs": "^6.5.2",
    "regenerator-runtime": "^0.13.7",
    "snakecase-keys": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "axios-mock-adapter": "^1.19.0",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/JestSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods"
    ],
    "ignore": [
      "node_modules",
      "tmp",
      ".git",
      "lib"
    ]
  }
}
